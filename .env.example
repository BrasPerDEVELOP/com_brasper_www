# Tema: claro (false) u oscuro (true)
VITE_DARK=

# Usar HTTPS (true) o HTTP (false)
VITE_SSL=

# Dominio de la API (sin protocolo)
VITE_DOMAIN=
# VITE_DOMAIN=
# VITE_DOMAIN=

# Prefijo de ruta opcional (empresa/tenant)
# VITE_COMPANY=

# País por defecto
VITE_COUNTRY=

# Prellenar login (solo desarrollo; no usar en producción)
VITE_USERNAME=user@example.com
VITE_PASSWORD=your_password_here

# Redirección para usuarios admin (URL externa)
# Ejemplo: https://admin.tudominio.com/sso
VITE_ADMIN_REDIRECT_URL=

# Secreto para cifrar el payload de SSO admin
VITE_ADMIN_REDIRECT_SECRET=

# Nivel de logs: debug | info | warn | error (en producción por defecto: warn)
# VITE_LOG_LEVEL=debug
